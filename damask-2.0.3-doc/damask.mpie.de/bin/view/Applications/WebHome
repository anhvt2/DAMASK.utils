jQuery(function($) {
  $('#newWikiAppForm').bind('submit', function() {
    var newweb = $('#newweb').val();
    if (!newweb.match(/^Applications\./)) {
      newweb = 'Applications.'+newweb;
      $('#newweb').val(newweb);
    }
    if(typeof(foswikiStrikeOne) == 'function') {
      foswikiStrikeOne(this);
    }
  });
});
