<h2 id="Atom_arrangement">  Atom arrangement  </h2>
<p></p>

<div style="width:50%; float:right; background:#f4fcf4; margin: 8pt 0pt 8pt 16pt;">
<p></p>

<a name="fig:bcc lattice"></a><table width="100%" border="0">
<tr><td><td align="center">
<center>
<x3d xmlns="http://www.web3d.org/specifications/x3d-3.0.xsd" height="300px" style="border:0px">
  <scene>
    <transform translation="-1 -1 -1">
      <shape DEF='atom'>
        <appearance>
          <material diffuseColor='gray' transparency='0.1' specularColor='gray'></material>  
        </appearance>
        <sphere radius='0.866'></sphere>
      </shape>
    </transform>
<p></p>

    <transform translation="1 -1 -1">
      <shape USE='atom' />
    </transform>
<p></p>

    <transform translation="-1 1 -1">
      <shape USE='atom' />
    </transform>
<p></p>

    <transform translation="1 1 -1">
      <shape USE='atom' />
    </transform>
<p></p>

    <transform translation="0 0 0">
      <shape USE='atom' />
    </transform>
<p></p>

    <transform translation="-1 -1 1">
      <shape USE='atom' />
    </transform>
<p></p>

    <transform translation="1 -1 1">
      <shape USE='atom' />
    </transform>
<p></p>

    <transform translation="-1 1 1">
      <shape USE='atom' />
    </transform>
<p></p>

    <transform translation="1 1 1">
      <shape USE='atom' />
    </transform>
<p></p>

   <viewpoint id="aView" centerOfRotation="0 0 0" position="0 0 8" />
<p></p>

  </scene>
</x3d>
