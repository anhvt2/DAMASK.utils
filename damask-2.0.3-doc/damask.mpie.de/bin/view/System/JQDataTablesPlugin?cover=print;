   }; return css&#91;data.TopicClassification.raw&#93;;' data-order='[[1,"asc"]]' data-columns='[{"visible":true,"render":{"_":"raw","display":"display"},"name":"Topic","orderable":false,"data":"Topic"},{"title":"Summary","visible":true,"name":"TopicSummary","render":{"_":"raw","display":"display"},"orderable":false,"data":"TopicSummary"},{"orderable":false,"data":"TopicClassification","render":{"_":"raw","display":"display"},"name":"TopicClassification","title":"Category","visible":true},{"orderable":false,"data":"Changed","render":{"display":"display","sort":"epoch","_":"raw"},"name":"Changed","visible":true}]' data-server-side='true' data-ajax='{"url":"https://damask.mpie.de/bin/rest/JQDataTablesPlugin/connector","data":{"connector":"search","form":"","t":1627669855,"webs":"System","query":"form~&#39;FAQForm&#39; AND topic!&#61;&#39;FAQTemplate&#39;","topic":"System.JQDataTablesPlugin"},"type":"post"}'>
