jQuery(function($) {
  $("#twistyDemo1").bind("afterOpen.twisty", function() { 
    alert("twisty is open");
  });
  $("#twistyDemo1").bind("afterClose.twisty", function() { 
    alert("twisty is closed");
  });
});
</script>

