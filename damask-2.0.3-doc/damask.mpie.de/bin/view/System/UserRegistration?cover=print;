<script class='script NATSKIN::VALIDATE::USERREGISTATION' type='text/javascript'>
jQuery(function($) {
  $('#Fwk1WikiName').wikiword({
	 source: '#Fwk1FirstName, #Fwk1LastName',
	 transliterate: true
  });
  var $form = $('#registrationForm')
	 .bind('invalid-form.validate', function(e, validator) {
		var errors = validator.numberOfInvalids();
		if (errors) {
		  $('#summary').html('There was an error. Please, see above.').show();
		} else {
		  $('#summary').hide();
		}
	 })
	 .validate({
		rules: {
		  Fwk1FirstName: 'required',
		  Fwk1LastName: 'required',
		  Fwk1WikiName: {
			 required: true,
			 wikiword: true,
			 remote: '/bin/rest/RenderPlugin/template?name=UserRegistrationView;expand=checkWikiName'
		  },
		  Fwk1Email: {
			 required: true,
			 email: true
		  },
		  Fwk1LoginName: {
			 required: true,
			 minlength: 2,
			 remote: '/bin/rest/RenderPlugin/template?name=UserRegistrationView;expand=checkLoginName'
		  },
		  Fwk1Password: {
			 required: true,
			 minlength: '7'
		  },
		  Fwk1Confirm: {
			 required: true,
			 equalTo: '#Fwk1Password'
		  }
		},
		messages: {
		  Fwk1FirstName: 'Enter your first name',
		  Fwk1LastName: 'Enter your last name',
		  Fwk1LoginName: {
			 required: 'Enter a login name',
			 minlength: $.format('Enter at least {0} characters'),
			 remote: $.format('LoginName {0} is already in use')
		  },
		  Fwk1WikiName: {
			 required: 'Enter a WikiName',
			 wikiword: $.format('Enter a valid WikiWord'),
			 remote: $.format('WikiName {0} is already in use')
		  },
		  Fwk1Password: {
			 required: 'Provide a password'
		  },
		  Fwk1Confirm: {
			 required: 'Repeat your password',
			 equalTo: 'Enter the same password as above'
		  },
		  Fwk1Email: {
			 required: 'Please enter a valid e-mail address'
		  }
		}
	 });
});
</script>
