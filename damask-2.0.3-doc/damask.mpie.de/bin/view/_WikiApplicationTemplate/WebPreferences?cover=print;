<p></p>

<h2 id="Appearance">  Appearance </h2>
<p></p>
 <ul>
<li> Set WEBBGCOLOR = #E2DCC8
</li> <li> Set WEBSUMMARY = 
</li> <li> Set SITEMAPLIST = off
</li></ul> 
<p></p>

<h2 id="Access_control">  Access control </h2>
<p></p>
 <ul>
<li> Set #DENYWEBVIEW =
</li> <li> Set #ALLOWWEBVIEW = 
</li> <li> Set DENYWEBCHANGE = <a href="/bin/view/Main/WikiGuest">WikiGuest</a>
</li> <li> Set ALLOWWEBCHANGE = <a href="/bin/view/Main/AdminGroup">AdminGroup</a>
</li> <li> Set ALLOWWEBRENAME = <a href="/bin/view/Main/AdminGroup">AdminGroup</a>
</li> <li> Set DENYWEBCOMMENT = <a href="/bin/view/Main/WikiGuest">WikiGuest</a>
</li></ul> 
<p></p>
 <ul>
<li> Set ALLOWTOPICCHANGE = <a href="/bin/view/Main/AdminGroup">AdminGroup</a>
</li> <li> Set ALLOWTOPICRENAME = <a href="/bin/view/Main/AdminGroup">AdminGroup</a>
</li></ul> 
<p></p>

<h2 id="Wiki_application_settings">  Wiki application settings </h2>
<p></p>
 <ul>
<li> Set WEBFORMS = <a href="/bin/view/Applications/WikiTopic">WikiTopic</a>
</li> <li> Set WEBLEXICON = <a href="/bin/view/Applications/WebLexicon">WebLexicon</a>
</li> <li> Set WEBMETADATA = <a href="/bin/view/Applications/LexiconForm">LexiconForm</a>
</li></ul> </div>
<p></p>

