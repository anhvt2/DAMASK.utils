atol_resistance         1
atol_shear              0
atol_twinfrac           0

n_twin                  2.500000000000e+01
n_slip                  5.500000000000e+00

gdot0_twin              0.001
gdot0_slip              0.001

twin_b                  0
twin_c                  0
twin_d                  20
twin_e                  20

